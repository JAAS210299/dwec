{
  "compilerOptions": {
    "target": "ES2023",                      // O "ESNext"
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true                 // Carpeta de salida
  }
}
